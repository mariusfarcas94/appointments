# MotionCare Appointments - Environment Configuration
# Copy this file to .env and update the values as needed

# Google Calendar Configuration
# Path to your Google Calendar service account key JSON file
# Leave empty to use Application Default Credentials (ADC)
GOOGLE_CALENDAR_SERVICE_ACCOUNT_KEY_PATH=

# Calendar ID to use (e.g., user@domain.com or a calendar's ID)
GOOGLE_CALENDAR_CALENDAR_ID=3642414e845beb4296235e98abe590fefb0fc46a0414b5b9131455b80b6fb808@group.calendar.google.com

# JWT Configuration
# Secret key for JWT token signing (use a strong, random key in production)
JWT_SECRET=mySecretKey123456789012345678901234567890

# JWT token expiration time in milliseconds (24 hours = 86400000)
JWT_EXPIRATION=86400000

# Database Configuration (usually no need to change these)
MYSQL_ROOT_PASSWORD=root123
MYSQL_DATABASE=motioncare_appointments
MYSQL_USER=motioncare
MYSQL_PASSWORD=motioncare123

# Application Configuration
# Default working hours
APPOINTMENTS_DEFAULT_WORKING_HOURS_START=08:00
APPOINTMENTS_DEFAULT_WORKING_HOURS_END=19:00

# CORS Configuration (add your frontend URLs here)
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001,http://localhost:4200,http://localhost:8080,http://localhost:5173,http://localhost:5174,http://127.0.0.1:3000,http://127.0.0.1:3001,http://127.0.0.1:4200,http://127.0.0.1:8080,http://127.0.0.1:5173,http://127.0.0.1:5174
